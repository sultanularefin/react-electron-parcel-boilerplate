{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.svg": [
    "...",
     "@parcel/transformer-svg-react"
     ],
     "*.js": [
     "@parcel/transformer-babel",
      "..."
      ]
  },
   "optimizers": {
      "*.js": ["@parcel/optimizer-esbuild"]
    },
    "reporters": [
    "...",
    "@parcel/reporter-sourcemap-visualiser"
    ],
     "validators": {
        "*.{js,jsx,ts,tsx}": [
        "@parcel/validator-eslint",
        "@parcel/validator-typescript"
        ]
      }

}
